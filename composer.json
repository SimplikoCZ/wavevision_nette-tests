{
  "name": "wavevision/nette-tests",
  "description": "PHPUnit extension for testing Nette applications.",
  "type": "library",
  "authors": [
    {
      "name": "Wavevision s.r.o.",
      "email": "info@wavevision.com"
    },
    {
      "name": "Jakub Filla",
      "email": "jakub.filla@wavevision.cz"
    },
    {
      "name": "Vít Rozsíval",
      "email": "vit@wavevision.com"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Wavevision\\NetteTests\\": "src/NetteTests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wavevision\\NetteTests\\TestApp\\": "tests/app/src/",
      "Wavevision\\NetteTests\\TestAppTests\\": "tests/app/tests/"
    }
  },
  "require": {
    "php": ">=7.2",
    "nette/application": "^3.0",
    "nette/bootstrap": "^3.0",
    "nette/caching": "3.0",
    "nette/di": "^3.0",
    "nette/finder": "^2.5",
    "nette/forms": "^3.0",
    "nette/http": "^3.0",
    "nette/utils": "^3.0",
    "latte/latte": "^2.5",
    "tracy/tracy": "^2.6",
    "phpunit/phpunit": "^8.4"
  },
  "require-dev": {
    "wavevision/coding-standard": "^1.1.0",
    "phing/phing": "^2.16",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "phpstan/phpstan": "^0.11.16",
    "phpstan/phpstan-phpunit": "^0.11.2",
    "php-coveralls/php-coveralls": "^2.1",
    "phpstan/phpstan-nette": "^0.11.1",
    "phpstan/extension-installer": "^1.0"
  },
  "scripts": {
    "phing": "phing"
  },
  "extra": {
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    }
  }
}
